/*26- Fazer um programa para ler as coordenadas x e y de dois pontos e calcular a distancia entre os dois
pontos no plano.*/
#include <stdio.h>
#include <math.h>
int main()
{ 
   int x1, y1, x2, y2, d; 
   puts("Informe dois valores inteiros da coordenada x1 e y1:");
   scanf("%d%d", &x1, &y1);
   puts("Informe dois valores inteiros da coordenada x2 e y2:");
   scanf("%d%d", &x2, &y2);
   d = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));
   printf("A distancia entre esses dois pontos equivale a: %d", d);
   return(0);
}
