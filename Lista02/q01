/*1- Diga a ordem de calculo dos operadores em cada uma dasinstrucoes em C a seguir e mostre o valor
de x depois que cada instrucao for executada.*/
#include <stdio.h>

int main()
{ 
    float x;
    x = 7 + 3 * 6 / 2 - 1; //1ª: 3*6 ; 2ª: 18/2 ; 3ª: 7 + 9 - 1
    printf("%.2f\n", x);
    x = 2 % 2 * 2 - 2 / 2; //1ª: 2%2 ; 2ª: 0*2 ; 3ª: 2/2 ; 4ª: 0 - 1
    printf("%.2f\n", x);
    x = (3 * 9 * (3 + (9*3/(3)))); //1ª: (3*9)/3 ; 2ª: 3 + 9 ; 3ª: 3 * 9 * 12
    printf("%.2f", x);
    
   return (0);
    
}
